# Talon voice commands for google drive

os: windows
tag: browser
browser.host: drive.google.com
#win.title: /Google Drive/
-

# navigation
select: key(x)
open: key(enter)
move: key(z)
rename: key(n)
share: key(.)

# files
[new] dock: key(shift-t)
[new] sheet: key(shift-s)
[new] folder: key(shift-f)
[new] form: key(shift-o)